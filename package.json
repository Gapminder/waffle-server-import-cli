{
  "name": "waffle-server-import-cli",
  "version": "2.5.25",
  "description": "WS import CLI",
  "main": "index.api.js",
  "bin": {
    "ws-cli": "index_cli.js"
  },
  "scripts": {
    "lint": "tslint --type-check steps.v2/**/*.ts --project tsconfig.json",
    "test": "npm run spec",
    "spec": "mocha --reporter mochawesome --reporter-options reportDir=specReports,reportFilename=specReport --timeout 10000 'test/**/*.spec.js'",
    "spec:coverage": "rm -rf specReports && istanbul --include-all-sources cover _mocha -- -R spec 'test/unit/**/*.spec.js'",
    "stop-track-config": "git update-index --assume-unchanged ./config/repositories.json ./config/waffle-server.json",
    "start": "npm run stop-track-config && node index.js",
    "api:import-update": "node cli.import-update.js",
    "api:set-default": "node cli.set-default.js",
    "api:diff": "node cli.generate-diff.js",
    "tsc": "tsc --project tsconfig.json"
  },
  "pre-commit": [
    "tsc",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/valor-software/waffle-server-import-cli.git"
  },
  "author": "Valor-Software",
  "license": "GPL-3.0",
  "contributors": [
    {
      "name": "Vyacheslav Panchenko",
      "email": "vyacheslav.panchenko@valor-software.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/valor-software/waffle-server-import-cli/issues"
  },
  "homepage": "https://github.com/valor-software/waffle-server-import-cli#readme",
  "dependencies": {
    "@types/async": "2.0.40",
    "@types/inquirer": "0.0.32",
    "@types/lodash": "^4.14.62",
    "@types/node": "6.0.61",
    "JSONStream": "1.3.1",
    "async": "2.3.0",
    "csvtojson": "1.1.4",
    "ddf-validation": "~1.2.4",
    "@types/node-fetch": "^1.6.7",
    "@types/rx": "^4.1.1",
    "git-csv-diff": "1.3.0",
    "highland": "2.10.5",
    "inquirer": "1.0.2",
    "ip-address": "5.8.8",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "moment-duration-format": "1.3.0",
    "router": "1.3.0",
    "shelljs": "0.7.7",
    "simple-git": "1.70.0",
    "superagent": "3.5.2",
    "ts-node": "3.0.2",
    "typescript": "2.2.2"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/mocha": "2.2.40",
    "@types/proxyquire": "1.3.27",
    "@types/sinon": "1.16.36",
    "chai": "3.5.0",
    "chai-sinon": "2.8.1",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mochawesome": "2.0.5",
    "pre-commit": "^1.2.2",
    "proxyquire": "1.7.11",
    "remap-istanbul": "^0.9.5",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "sinon-chai-in-order": "0.1.0",
    "sinon-test": "1.0.1",
    "tslint": "^5.1.0"
  }
}
